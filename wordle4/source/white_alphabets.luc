module white_alphabets ( // contains the inputs thare are to be fed into the alu via the fsm
    input selector[5],   
    output out[25][24] // output an array
  ) {
  
  const INPUTS = $reverse({
    { // turned off
    h000000, h000000, h000000, h000000, h000000,
    h000000, h000000, h000000, h000000, h000000,
    h000000, h000000, h000000, h000000, h000000,
    h000000, h000000, h000000, h000000, h000000,
    h000000, h000000, h000000, h000000, h000000
    },
    { // A
    h000000, h000000, h111111, h000000, h000000,
    h000000, h111111, h000000, h111111, h000000,
    h111111, h000000, h000000, h000000, h111111,
    h111111, h111111, h111111, h111111, h111111,
    h111111, h000000, h000000, h000000, h111111
    }
  });

  always {
    out = INPUTS[selector];
  }
}