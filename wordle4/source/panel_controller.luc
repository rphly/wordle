module panel_controller (
    input clk,  // clock
    input rst,  // reset

    input clear,
    input check,
    
    output is_pressed,
    output out[5]
  ) {
  .clk(clk) {
    button clear_(.button_input(clear));
    button check_(.button_input(check));
   }
  
  always {
    if (clear_.out) {
      out = b11111;
      is_pressed = 1;
    } else if (check_.out) {
      out = b11110;
      is_pressed = 1;
    } else {
      out = 5b0;
      is_pressed = 0;
    }
  }
}